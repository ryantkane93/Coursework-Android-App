<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.bcs421ryankanehwk5courseworkapp2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CourseWorkListActivity"></activity>
        <activity android:name=".CourseWorkItemActivity"></activity>
        <activity android:name=".AddCourseWorkItemActivity"></activity>
        <activity android:name=".DropGradePreferenceActivity"></activity>
        <provider android:name=".AssignmentContentProvider"
            android:authorities="com.ryankane.assignmentcontentprovider"
            android:exported="true"/>
    </application>

</manifest>
